{
  "expo": {
    "name": "My-UAE",
    "slug": "my-uae",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "bundleIdentifier": "com.myUae.test",
      "supportsTablet": true,
      "infoPlist": {
        "ExpoLocalization_supportsRTL": true
      }
    },
    "android": {
      "package": "com.myUae.test"
    },
    "extra": {
      "eas": {
        "projectId": "3448c505-bee0-44a5-a3d6-f7099e5e86d9"
      },
      "supportsRTL": true
    },
    "assetBundlePatterns": [
      "src/assets/locales/de.json",
      "src/assets/locales/en.json",
      "src/assets/icons/*",
      "src/assets/img/*"
    ],
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "georgealtawil"
  }
}
